{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Clubs": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/clubs",
      "sourceRoot": "projects/clubs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/clubs/src/main.ts",
            "tsConfig": "projects/clubs/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/clubs/public"
              },
              {
                "glob": "**/*",
                "input": "projects/clubs/src/assets"
                
              }
            ],
            "styles": [
              "projects/clubs/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Clubs:build:production"
            },
            "development": {
              "buildTarget": "Clubs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/clubs/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/clubs/public"
              }
            ],
            "styles": [
              "projects/clubs/src/styles.css"
            ]
          }
        }
      }
    },
    "study": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/study",
      "sourceRoot": "projects/study/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            
            "browser": "projects/study/src/main.ts",
            "tsConfig": "projects/study/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/study/public"
              }
            ],
            "styles": [
              "projects/study/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "study:build:production"
            },
            "development": {
              "buildTarget": "study:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/study/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/study/public"
              }
            ],
            "styles": [
              "projects/study/src/styles.css"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
